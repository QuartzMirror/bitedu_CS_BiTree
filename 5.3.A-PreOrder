int i=1;
ElemType PreOrder(BiTree b,int k){
  if(b==NULL)
    return '#';
  if(i==k)
    return b->data;
  i++;
  ch=PreNode(b->lchild,k);
  if(ch!='#')
    return ch;
  ch=PreOrder(b->rchild,k);
    return ch;
}

//update
void PreOrder(BiTree T,int k){
	InitStack(S);
	BiTree p=T;
	int count=0;
	while(!IsEmpty(S)||p!=NULL){
		if(p!=NULL){
			count++;
			if(count==k)
				visit(p);
			Push(S,p);
			p=p->lchild;
		}
		else{
			Pop(S,p);
			p=p->rchild;
		}
	}
}
