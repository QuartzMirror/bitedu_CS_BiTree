void PreOrder(BiTree T,int k){
	InitStack S;
	BiTree p=T;
	int i=0;
	Push(S,p);
	if(p!=NULL&&!IsEmpty(S)){
		Pop(S,p);
		i++;
		if(i==k)
			return p->data;
		if(p->lchild!=NULL)
			Push(p->lchild);
		if(p->rchild!=NULL)
			Push(p->rchild);
	}
}

