void BiTreeDepth(BiTree T){
	InitQueue(Q);
	Queue p=T;
	int count,Dp=0;
	EnQueue(Q,T);
	count++;
	if(T!=NULL){
		while(IsEmpty(Q)!=NULL){
			DeQueue(Q,p);
			count--;
			if(count==0)
				Dp++;
			visit(p);
			if(p->lchild!=NULL){
				EnQueue(Q,p->lchild);
				count++;
			}
			if(p->rchild!=NULL){
				EnQueue(Q,p->rchild);
				count++;
			}
		}
	}
}

//参照范例 本题还有rear front last level等复杂的判断
//以实现树的深度求值
//还是挺有意思的
