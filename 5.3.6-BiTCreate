void CreateBiTree(int A[],int B[],int l1,int l2,int h1,int h2){
	if(l1<h1||l2<h2)
		return false;
	int llen,rlen;
	int i,j,k=0;
	int p=(BiTree*)malloc(sizeof(BiTree));
	p->data=A[l1];
	while(B[i]!=p->data)
		i++;
	llen=i-l1;
	rlen=h1-i;
	CreateBiTree(A[],B[],l1+1,l2,l1+llen-1,l2+llen-1);
	CreateBiTree(A[],B[],h1-rlen,h2-rlen,h1,h2;
}
