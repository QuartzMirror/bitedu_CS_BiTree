void ReverseBiTree(BiTree T){
	if(T==NULL)
		return 0;
	ElemType temp=T->lchild;
	T->lchild=T->rchild;
	T->rchild=temp;
	ReverseBiTree(p->lchild);
	ReverseBiTree(p->rchild);
}

